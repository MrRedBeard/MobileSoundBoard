<?xml version='1.0' encoding='utf-8'?>
<widget id="com.x2.bmf" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BMF</name>
    <description>
        BMF Soundboard
    </description>
    <author email="britton@x2labs.com" href="http://www.x2labs.com">
        X2Labs Development Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
